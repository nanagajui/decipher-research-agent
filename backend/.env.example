
# Bright Data credentials
BRIGHT_DATA_API_TOKEN=your_bright_data_api_token
BRIGHT_DATA_BROWSER_AUTH=your_bright_data_browser_auth

# Database connection URL - use localhost for local PostgreSQL
DATABASE_URL=postgresql://decipher:yoursecurepassword@localhost:5432/decipher_db

# OpenRouter API key
OPENROUTER_API_KEY=your_openrouter_api_key

# OpenAI API key
OPENAI_API_KEY=your_openai_api_key

# LangTrace API key
LANGTRACE_API_KEY=your_langtrace_api_key

# LemonFox AI API key
LEMONFOX_API_KEY=your_lemonfox_api_key

# Qdrant vector database configuration - use localhost when running Qdrant in Docker
QDRANT_API_KEY=your_qdrant_api_key
